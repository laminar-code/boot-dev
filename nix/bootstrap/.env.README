# Use this template to create a .env file for docker compose

USERNAME=<username of dev user>

GIT_UTILS_REPO=<repo containing nix config and flakes>
GIT_BEARER_TOKEN=<bearer token for git repo>
GIT_UTILS_REPO_RAW=<raw access to repo containing nix config and flakes>
GIT_USER_NAME=<full name of git user>
GIT_USER_EMAIL=<email address for same user>

LOCAL_DIRECTORY=<directory volume on host to mount at /mnt/host in the container>

HOME_NIX=<raw path within git utils repo for home.nix>

